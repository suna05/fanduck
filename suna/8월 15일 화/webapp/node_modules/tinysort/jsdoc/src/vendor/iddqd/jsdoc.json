{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": [ "src", "README.md" ],
		"exclude": [ "myProject/lib/ignore.js" ],
		"includePattern": "iddqd.+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": [],
	"nogins": [
		"plugins/markdown"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"systemName": "iddqd",
		"footer": "",
		"navType": "vertical",
		"theme": "sjeiti",
		"linenums": true,
		"collapseSymbols": false,
		"inverseNav": true,
		"copyright": "Ron Valstar <ron@ronvalstar.nl>"
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	},
	"opts": {
		"template": "jsdoc/template",
		"tutorials": "jsdoc/tutorials",
		"destination": "doc",
		"recurse": true,
		"private": false
	}
}