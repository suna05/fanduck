{
"update_url": "https://clients2.google.com/service/update2/crx",

    "version": "7.2.10",
    "manifest_version": 2,
    "name": "__MSG_app_name__",
    "description": "__MSG_app_desc__",
    "default_locale": "en",

    "icons": {
        "128": "/css/images/chromeapp_icon_128.png",
        "96": "/css/images/chromeapp_icon_96.png",
        "48": "/css/images/chromeapp_icon_48.png",
        "16": "/css/images/chromeapp_icon_16.png"
    },

    "app": {
        "background": {
            "scripts": ["js/background.js"]
        }
    },
    "permissions": [
        "fileSystem",
        "fileSystem.write",
        "notifications",
        "storage",
        "unlimitedStorage",
        "clipboardRead",
        "geolocation",
        "http://*/",
        "https://*/",
        "https://chart.googleapis.com/",
        "identity",
        "identity.email",
        "gcm",
        "webview"
    ]
}
