{
    "manifest_version": 2,

    "name": "fanduck",
    "description": "테스트입니다.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "images/logo.png",
        "default_popup": "popup.html"
    },
    "icons" : {
      "64" : "images/logo.png",
      "32" : "images/logo.png",
      "16" : "images/logo.png",
      "128" : "images/logo.png"
    },
    "permissions": [
     "tabs",
     "contextMenus",
     "<all_urls>",
     "http://www.kobis.or.kr/",
     "notifications",
     "http://www.twitter.com/i/notifications"
     ],
     "content_scripts" : [
       {
         "matches" : ["http://*/*","https://*/*"],
         "css" : ["css/bootstrap.min.css"],
         "js" : [
           "js/jquery.js",
           "js/login.js",
           "js/logout.js"
           ]
       }
     ],
     "background" : {
        "page" : "background.html"
      }
}
